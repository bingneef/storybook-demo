{
  "name": "test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aframe": "^0.8.2",
    "aframe-animation-component": "^5.1.2",
    "babel-loader": "^8.0.4",
    "bootstrap": "^4.1.3",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.6",
    "raw-html-react": "^1.2.3",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "styled-components": "^4.1.2",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dev:test": "react-scripts test",
    "deploy:build": "yarn build && now build --public",
    "test": "yarn tsc && react-scripts test",
    "test:integration": "jest -c integration/jest.config.js",
    "docker:test": "wait-on -l http-get://storybook:9009 && STORYBOOK_URL=http://storybook:9009 yarn test",
    "docker:test:integration": "wait-on -l http-get://static_build:5000 && APP_URL=http://static_build:5000 yarn test:integration",
    "ci:test": "wait-on -l http-get://storybook:9009 && CI=true STORYBOOK_URL=http://storybook:9009 yarn test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009",
    "build:storybook": "build-storybook -s public",
    "deploy:storybook": "yarn build:storybook && now storybook-static --public",
    "typings": "typings/index",
    "now-build": "react-scripts build && mv build dist"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-info": "^4.0.9",
    "@storybook/addon-links": "^4.0.9",
    "@storybook/addon-storyshots": "^4.0.9",
    "@storybook/addon-storyshots-puppeteer": "^4.0.9",
    "@storybook/react": "^4.0.9",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^4.1.0",
    "jest-puppeteer": "^3.6.0",
    "node-sass": "^4.10.0",
    "now": "^12.1.8",
    "puppeteer": "^1.11.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-test-renderer": "^16.6.3",
    "serve": "^10.1.1",
    "storybook": "^1.0.0",
    "wait-on": "^3.2.0"
  }
}
